<div class="page-header">
    <h1>{{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}</h1>
</div>

<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST"
      class="fos_user_resetting_reset">
    {{ form_widget(form) }}
    <div>
        <input class="btn btn-primary" type="submit"
               value="{{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
</form>
